<!DOCTYPE html>
<html lang="en">
<head>
<%- include('../partials/head'); %>
</head>
<body class="container">

    <header>
        <%- include('../partials/header'); %>
    </header>

    <main>
        <div class="jumbotron">
            <p><%= tagline %></p>
            <h1>Users List</h1>
            <ul>
                <% users.forEach(function(user) { %>
                    <li>
                        <strong>Name : <%= user.name %> , Age : <%= user.age %></strong>
                    </li>
                <% }); %>
            </ul>
        </div>
    </main>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>

</body>
</html>